{
  "name": "client-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite --config ./config/vite.config.ts",
    "build": "vite build --mode staging --config ./config/vite.config.ts",
    "serve": "vite preview --port 8080",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,yml}\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "g:icons": "ts-node -r tsconfig-paths/register scripts/icons-gen.js",
    "g:page": "ts-node -r tsconfig-paths/register scripts/page-gen.js",
    "g:rsc": "ts-node -r tsconfig-paths/register scripts/component-gen.js",
    "generate": "graphql-codegen --config --watch codegen.yml"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.6.9",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "antd": "^4.21.7",
    "classnames": "^2.3.1",
    "graphql": "^16.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "moment": "^2.29.3",
    "mongodb": "^4.8.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "react-svg": "^14.1.8",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.9.1",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/near-operation-file-preset": "^2.4.0",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-document-nodes": "2.3.2",
    "@graphql-codegen/typescript-mongodb": "2.4.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.2",
    "@graphql-codegen/typescript-resolvers": "2.7.2",
    "@types/eslint": "8.2.1",
    "@types/graphql": "^14.5.0",
    "@types/node": "16.11.12",
    "@types/node-cron": "^3.0.1",
    "@types/react": "^18.0.0",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "18.0.0",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^2.5.1",
    "ts-node": "10.4.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.5.3",
    "vite": "^3.0.2",
    "vitest": "^0.5.8"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
